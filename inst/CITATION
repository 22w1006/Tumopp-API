citHeader("To cite tumopp/tumorr in publications use:")

citEntry(entry = "Article",
  title        = "Simulation Framework for Generating Intratumor Heterogeneity Patterns in a Cancer Cell Population",
  author       = personList(as.person("Watal M. Iwasaki"), as.person("Hideki Innan")),
  journal      = "bioRxiv",
  year         = "2017",
  volume       = "preprint",
  url          = "http://www.biorxiv.org/content/early/2017/02/19/109801",

  textVersion  = paste("W. M. Iwasaki and H. Innan (2017).",
                 "Simulation Framework for Generating Intratumor Heterogeneity Patterns in a Cancer Cell Population.",
                 "bioRxiv preprint")
)
